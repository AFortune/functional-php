<package version="2.0">
	<name>functional</name>
    <uri>https://github.com/lstrojny/functional-php</uri>
	<summary>Functional primitives for PHP</summary>
	<description> number of functional primitives like map(), select(), each() for the PHP language</description>
	<lead>
		<name>Lars Strojny</name>
		<user>lstrojny</user>
		<email>lstrojny@php.net</email>
		<active>yes</active>
	</lead>
	<date>2011-06-10</date>
	<version>
		<release>0.1dev</release>
		<api>0.1</api>
	</version>
	<stability>
		<release>alpha</release>
		<api>alpha</api>
	</stability>
	<license uri="http://www.opensource.org/licenses/bsd-license.php">BSD License</license>
	<notes>
- Initial release
	</notes>
	<contents>
		<dir name="/">
			<file name="config.m4" role="src"/>
			<file name="config.w32" role="src"/>
			<file name="functional.c" role="src"/>
			<file name="php_functional.h" role="src"/>
		</dir>
	</contents>
	<dependencies>
		<required>
			<php>
				<min>5.3</min>
			</php>
			<pearinstaller>
				<min>1.4.3</min>
			</pearinstaller>
		</required>
	</dependencies>
    <providesextension>functional</providesextension>
	<extsrcrelease/>
